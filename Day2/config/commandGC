gcloud compute instances create nginx-gcloud \
	--zone=us-central1-c \
	--custom-cpu=1 \
	--custom-vm-type=n1 \
	--custom-memory=4608MB \
	--boot-disk-type=pd-ssd \
	--boot-disk-device-name nginx-console \
	--boot-disk-size=35GB \
	--image=centos-7 \
	--labels servertype=nginxserver,osfamily=redhat,wayofinstallation=gcloud \
	--tags http-server,https-server \
	--deletion-protection \
	--metadata-from-file startup-script=script.sh
